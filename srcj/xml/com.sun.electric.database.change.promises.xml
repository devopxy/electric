<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="com.sun.electric.change">
	<promise keyword="promise" contents="&quot;requiresColor DBChanger;&quot; for kill*(**) | modify*(**) | erase*(**) | write*(**)"/>
	<promise keyword="promise" contents="&quot;requiresColor (DBExaminer | DBChanger);&quot; for get*(**) | new(**) | examine*(**)"/>
	<promise keyword="promise" contents="&quot;colorNotRelevant&quot; for add*Listener(**) | remove*Listener(**)"/>
	<promise keyword="@promise" contents="&quot;requiresColor DBChanger;&quot; for modify*(**)"/>
	<promise keyword="promise" contents="&quot;requiresColor (DBExaminer | DBChanger);&quot; for lowLevelGetUserBits(**)"/>
	<promise keyword="promise" contents="&quot;requiresColor DBChanger;&quot; for lowLevel*(**) &amp; (!(lowLevelGetUserBits(**)))"/>
</package>
